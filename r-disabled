[32m+[m	[32m/**[m
[32m+[m	[32m * Add machine_id to machine list.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param string $machine_id namchine_id for the job[m
[32m+[m	[32m * @access public[m
[32m+[m	[32m * @return bool true if the file could be added; false on error[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction add_machine_id($machine_id) {[m
[32m+[m		[32mif (!$machine_id) return false;[m
[32m+[m		[32mif (!($stmt = get_pdo()->prepare('SELECT * FROM machine WHERE machine_id = :machine_id'))) {[m
[32m+[m			[32mreturn null;[m
[32m+[m		[32m}[m
[32m+[m		[32m$stmt->bindParam(':machine_id', $machine_id);[m
[32m+[m		[32m$stmt->execute();[m
[32m+[m		[32m$row = $stmt->fetch(PDO::FETCH_ASSOC);[m
[32m+[m		[32mif (!$row) return null;[m
[32m+[m		[32m$this->fields['machines_ip'][] = $row['ip'];[m
[32m+[m		[32m$this->fields['machines'][$machine_id] = $row;[m
[32m+[m	[32m}[m
[32m+[m
