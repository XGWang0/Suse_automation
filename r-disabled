[32m+[m
[32m+[m	[32mfunction has_perm($machine_id,$perm) {[m
[32m+[m		[32mif(isset($this->fields['machines'][$machine_id])) {[m
[32m+[m			[32mif(preg_match("/$perm/i", $this->fields['machines'][$machine_id]['perm']))[m
