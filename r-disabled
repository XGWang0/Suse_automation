[32m+[m
[32m+[m	[32mprivate static function get_master_socket() {[m
[32m+[m		[32mif (is_null(Job::$master_socket)) {[m
[32m+[m			[32m$conf = ConfigFactory::build();[m
[32m+[m			[32mif (!(Job::$master_socket = fsockopen($conf->cmdline->host, $conf->cmdline->port))) {[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m			[32mstream_set_blocking(Job::$master_socket, false);[m
[32m+[m[41m		[m
[32m+[m			[32m$count = 0;[m
[32m+[m			[32mwhile (($s = fgets(Job::$master_socket, 4096)) != "$>") {[m
[32m+[m				[32mif (!$s) {[m
[32m+[m					[32mif (($count++) > 10) {[m
[32m+[m						[32mfclose(Job::$master_socket);[m
[32m+[m						[32mJob::$master_socket = null;[m
[32m+[m						[32mJob::$readerr = "Could not get the master command prompt. Giving up after 10 empty reads from master.";[m
[32m+[m						[32mreturn null;[m
[32m+[m					[32m}[m
[32m+[m					[32msleep(1);[m
[32m+[m					[32mcontinue;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn Job::$master_socket;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * send_job[m[41m [m
[32m+[m	[32m *[m
[32m+[m	[32m * Sends a job to the machine by the HAMSTA master commandline interface[m
[32m+[m	[32m *[m[41m [m
[32m+[m	[32m * @param string $filename Filename of the XML job description on the[m[41m [m
[32m+[m	[32m *	  master. This is not a local filename![m
[32m+[m	[32m *[m
[32m+[m	[32m * @access public[m
[32m+[m	[32m * @return bool true if the job could be send; false on error[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction send_job() {[m
[32m+[m		[32mif (!($sock = Job::get_master_socket())) {[m
[32m+[m			[32m$this->errmsg = (empty(Job::$readerr)?"cannot connect to master!":Job::$readerr);[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m
[32m+[m		[32mglobal $config;[m
[32m+[m		[32mif ($config->authentication->use) {[m
[32m+[m			[32m$user = User::getById (User::getIdent (), $config);[m
[32m+[m			[32mfputs ($sock, "log in " . $user->getLogin() . " "[m
[32m+[m			[32m       . $user->getPassword() . "\n");[m
[32m+[m			[32m$response = "";[m
[32m+[m			[32mwhile (($s = fgets($sock, 4096)) != "$>") {[m
[32m+[m				[32m$response .= $s;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!stristr ($response, "you were authenticated")) {[m
[32m+[m				[32m$this->errmsg = $response;[m
