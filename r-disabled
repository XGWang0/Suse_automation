[1mdiff --git a/external/jquery_treetable/jquery_treetable/jquery.treetable.js b/external/jquery_treetable/jquery_treetable/jquery.treetable.js[m
[1mnew file mode 100644[m
[1mindex 0000000..3a715d2[m
[1m--- /dev/null[m
[1m+++ b/external/jquery_treetable/jquery_treetable/jquery.treetable.js[m
[36m@@ -0,0 +1,620 @@[m
[32m+[m[32m/*[m
[32m+[m[32m * jQuery treetable Plugin 3.1.0[m
[32m+[m[32m * http://ludo.cubicphuse.nl/jquery-treetable[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright 2013, Ludo van den Boom[m
[32m+[m[32m * Dual licensed under the MIT or GPL Version 2 licenses.[m
[32m+[m[32m */[m
[32m+[m[32m(function() {[m
[32m+[m[32m  var $, Node, Tree, methods;[m
[32m+[m
[32m+[m[32m  $ = jQuery;[m
[32m+[m
[32m+[m[32m  Node = (function() {[m
[32m+[m[32m    function Node(row, tree, settings) {[m
[32m+[m[32m      var parentId;[m
[32m+[m
[32m+[m[32m      this.row = row;[m
[32m+[m[32m      this.tree = tree;[m
[32m+[m[32m      this.settings = settings;[m
[32m+[m
[32m+[m[32m      // TODO Ensure id/parentId is always a string (not int)[m
[32m+[m[32m      this.id = this.row.data(this.settings.nodeIdAttr);[m
[32m+[m
[32m+[m[32m      // TODO Move this to a setParentId function?[m
[32m+[m[32m      parentId = this.row.data(this.settings.parentIdAttr);[m
[32m+[m[32m      if (parentId != null && parentId !== "") {[m
[32m+[m[32m        this.parentId = parentId;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.treeCell = $(this.row.children(this.settings.columnElType)[this.settings.column]);[m
[32m+[m[32m      this.expander = $(this.settings.expanderTemplate);[m
[32m+[m[32m      this.indenter = $(this.settings.indenterTemplate);[m
[32m+[m[32m      this.children = [];[m
[32m+[m[32m      this.initialized = false;[m
[32m+[m[32m      this.treeCell.prepend(this.indenter);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Node.prototype.addChild = function(child) {[m
[32m+[m[32m      return this.children.push(child);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.ancestors = function() {[m
[32m+[m[32m      var ancestors, node;[m
[32m+[m[32m      node = this;[m
[32m+[m[32m      ancestors = [];[m
[32m+[m[32m      while (node = node.parentNode()) {[m
[32m+[m[32m        ancestors.push(node);[m
[32m+[m[32m      }[m
[32m+[m[32m      return ancestors;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.collapse = function() {[m
[32m+[m[32m      if (this.collapsed()) {[m
[32m+[m[32m        return this;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.row.removeClass("expanded").addClass("collapsed");[m
[32m+[m
[32m+[m[32m      this._hideChildren();[m
[32m+[m[32m      this.expander.attr("title", this.settings.stringExpand);[m
[32m+[m
[32m+[m[32m      if (this.initialized && this.settings.onNodeCollapse != null) {[m
[32m+[m[32m        this.settings.onNodeCollapse.apply(this);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.collapsed = function() {[m
[32m+[m[32m      return this.row.hasClass("collapsed");[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // TODO destroy: remove event handlers, expander, indenter, etc.[m
[32m+[m
[32m+[m[32m    Node.prototype.expand = function() {[m
[32m+[m[32m      if (this.expanded()) {[m
[32m+[m[32m        return this;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.row.removeClass("collapsed").addClass("expanded");[m
[32m+[m
[32m+[m[32m      if (this.initialized && this.settings.onNodeExpand != null) {[m
[32m+[m[32m        this.settings.onNodeExpand.apply(this);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if ($(this.row).is(":visible")) {[m
[32m+[m[32m        this._showChildren();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.expander.attr("title", this.settings.stringCollapse);[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.expanded = function() {[m
[32m+[m[32m      return this.row.hasClass("expanded");[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.hide = function() {[m
[32m+[m[32m      this._hideChildren();[m
[32m+[m[32m      this.row.hide();[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.isBranchNode = function() {[m
[32m+[m[32m      if(this.children.length > 0 || this.row.data(this.settings.branchAttr) === true) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return false;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.updateBranchLeafClass = function(){[m
[32m+[m[32m      this.row.removeClass('branch');[m
[32m+[m[32m      this.row.removeClass('leaf');[m
[32m+[m[32m      this.row.addClass(this.isBranchNode() ? 'branch' : 'leaf');[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.level = function() {[m
[32m+[m[32m      return this.ancestors().length;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.parentNode = function() {[m
[32m+[m[32m      if (this.parentId != null) {[m
[32m+[m[32m        return this.tree[this.parentId];[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return null;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.removeChild = function(child) {[m
[32m+[m[32m      var i = $.inArray(child, this.children);[m
[32m+[m[32m      return this.children.splice(i, 1)[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.render = function() {[m
[32m+[m[32m      var handler,[m
[32m+[m[32m          settings = this.settings,[m
[32m+[m[32m          target;[m
[32m+[m
[32m+[m[32m      if (settings.expandable === true && this.isBranchNode()) {[m
[32m+[m[32m        handler = function(e) {[m
[32m+[m[32m          $(this).parents("table").treetable("node", $(this).parents("tr").data(settings.nodeIdAttr)).toggle();[m
[32m+[m[32m          return e.preventDefault();[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        this.indenter.html(this.expander);[m
[32m+[m[32m        target = settings.clickableNodeNames === true ? this.treeCell : this.expander;[m
[32m+[m
[32m+[m[32m        target.off("click.treetable").on("click.treetable", handler);[m
[32m+[m[32m        target.off("keydown.treetable").on("keydown.treetable", function(e) {[m
[32m+[m[32m          if (e.keyCode == 13) {[m
[32m+[m[32m            handler.apply(this, [e]);[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.indenter[0].style.paddingLeft = "" + (this.level() * settings.indent) + "px";[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.reveal = function() {[m
[32m+[m[32m      if (this.parentId != null) {[m
[32m+[m[32m        this.parentNode().reveal();[m
[32m+[m[32m      }[m
[32m+[m[32m      return this.expand();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.setParent = function(node) {[m
[32m+[m[32m      if (this.parentId != null) {[m
[32m+[m[32m        this.tree[this.parentId].removeChild(this);[m
[32m+[m[32m      }[m
[32m+[m[32m      this.parentId = node.id;[m
[32m+[m[32m      this.row.data(this.settings.parentIdAttr, node.id);[m
[32m+[m[32m      return node.addChild(this);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.show = function() {[m
[32m+[m[32m      if (!this.initialized) {[m
[32m+[m[32m        this._initialize();[m
[32m+[m[32m      }[m
[32m+[m[32m      this.row.show();[m
[32m+[m[32m      if (this.expanded()) {[m
[32m+[m[32m        this._showChildren();[m
[32m+[m[32m      }[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype.toggle = function() {[m
[32m+[m[32m      if (this.expanded()) {[m
[32m+[m[32m        this.collapse();[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.expand();[m
[32m+[m[32m      }[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype._hideChildren = function() {[m
[32m+[m[32m      var child, _i, _len, _ref, _results;[m
[32m+[m[32m      _ref = this.children;[m
[32m+[m[32m      _results = [];[m
[32m+[m[32m      for (_i = 0, _len = _ref.length; _i < _len; _i++) {[m
[32m+[m[32m        child = _ref[_i];[m
[32m+[m[32m        _results.push(child.hide());[m
[32m+[m[32m      }[m
[32m+[m[32m      return _results;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype._initialize = function() {[m
[32m+[m[32m      var settings = this.settings;[m
[32m+[m
[32m+[m[32m      this.render();[m
[32m+[m
[32m+[m[32m      if (settings.expandable === true && settings.initialState === "collapsed") {[m
[32m+[m[32m        this.collapse();[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.expand();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (settings.onNodeInitialized != null) {[m
[32m+[m[32m        settings.onNodeInitialized.apply(this);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this.initialized = true;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Node.prototype._showChildren = function() {[m
[32m+[m[32m      var child, _i, _len, _ref, _results;[m
[32m+[m[32m      _ref = this.children;[m
[32m+[m[32m      _results = [];[m
[32m+[m[32m      for (_i = 0, _len = _ref.length; _i < _len; _i++) {[m
[32m+[m[32m        child = _ref[_i];[m
[32m+[m[32m        _results.push(child.show());[m
[32m+[m[32m      }[m
[32m+[m[32m      return _results;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return Node;[m
[32m+[m[32m  })();[m
[32m+[m
[32m+[m[32m  Tree = (function() {[m
[32m+[m[32m    function Tree(table, settings) {[m
[32m+[m[32m      this.table = table;[m
[32m+[m[32m      this.settings = settings;[m
[32m+[m[32m      this.tree = {};[m
[32m+[m
[32m+[m[32m      // Cache the nodes and roots in simple arrays for quick access/iteration[m
[32m+[m[32m      this.nodes = [];[m
[32m+[m[32m      this.roots = [];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Tree.prototype.collapseAll = function() {[m
[32m+[m[32m      var node, _i, _len, _ref, _results;[m
[32m+[m[32m      _ref = this.nodes;[m
[32m+[m[32m      _results = [];[m
[32m+[m[32m      for (_i = 0, _len = _ref.length; _i < _len; _i++) {[m
[32m+[m[32m        node = _ref[_i];[m
[32m+[m[32m        _results.push(node.collapse());[m
[32m+[m[32m      }[m
[32m+[m[32m      return _results;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.expandAll = function() {[m
[32m+[m[32m      var node, _i, _len, _ref, _results;[m
[32m+[m[32m      _ref = this.nodes;[m
[32m+[m[32m      _results = [];[m
[32m+[m[32m      for (_i = 0, _len = _ref.length; _i < _len; _i++) {[m
[32m+[m[32m        node = _ref[_i];[m
[32m+[m[32m        _results.push(node.expand());[m
[32m+[m[32m      }[m
[32m+[m[32m      return _results;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.findLastNode = function (node) {[m
[32m+[m[32m      if (node.children.length > 0) {[m
[32m+[m[32m        return this.findLastNode(node.children[node.children.length - 1]);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return node;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.loadRows = function(rows) {[m
[32m+[m[32m      var node, row, i;[m
[32m+[m
[32m+[m[32m      if (rows != null) {[m
[32m+[m[32m        for (i = 0; i < rows.length; i++) {[m
[32m+[m[32m          row = $(rows[i]);[m
[32m+[m
[32m+[m[32m          if (row.data(this.settings.nodeIdAttr) != null) {[m
[32m+[m[32m            node = new Node(row, this.tree, this.settings);[m
[32m+[m[32m            this.nodes.push(node);[m
[32m+[m[32m            this.tree[node.id] = node;[m
[32m+[m
[32m+[m[32m            if (node.parentId != null && this.tree[node.parentId]) {[m
[32m+[m[32m              this.tree[node.parentId].addChild(node);[m
[32m+[m[32m            } else {[m
[32m+[m[32m              this.roots.push(node);[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      for (i = 0; i < this.nodes.length; i++) {[m
[32m+[m[32m        node = this.nodes[i].updateBranchLeafClass();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.move = function(node, destination) {[m
[32m+[m[32m      // Conditions:[m
[32m+[m[32m      // 1: +node+ should not be inserted as a child of +node+ itself.[m
[32m+[m[32m      // 2: +destination+ should not be the same as +node+'s current parent (this[m
[32m+[m[32m      //    prevents +node+ from being moved to the same location where it already[m
[32m+[m[32m      //    is).[m
[32m+[m[32m      // 3: +node+ should not be inserted in a location in a branch if this would[m
[32m+[m[32m      //    result in +node+ being an ancestor of itself.[m
[32m+[m[32m      var nodeParent = node.parentNode();[m
[32m+[m[32m      if (node !== destination && destination.id !== node.parentId && $.inArray(node, destination.ancestors()) === -1) {[m
[32m+[m[32m        node.setParent(destination);[m
[32m+[m[32m        this._moveRows(node, destination);[m
[32m+[m
[32m+[m[32m        // Re-render parentNode if this is its first child node, and therefore[m
[32m+[m[32m        // doesn't have the expander yet.[m
[32m+[m[32m        if (node.parentNode().children.length === 1) {[m
[32m+[m[32m          node.parentNode().render();[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if(nodeParent){[m
[32m+[m[32m        nodeParent.updateBranchLeafClass();[m
[32m+[m[32m      }[m
[32m+[m[32m      if(node.parentNode()){[m
[32m+[m[32m        node.parentNode().updateBranchLeafClass();[m
[32m+[m[32m      }[m
[32m+[m[32m      node.updateBranchLeafClass();[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.removeNode = function(node) {[m
[32m+[m[32m      // Recursively remove all descendants of +node+[m
[32m+[m[32m      this.unloadBranch(node);[m
[32m+[m
[32m+[m[32m      // Remove node from DOM (<tr>)[m
[32m+[m[32m      node.row.remove();[m
[32m+[m
[32m+[m[32m      // Clean up Tree object (so Node objects are GC-ed)[m
[32m+[m[32m      delete this.tree[node.id];[m
[32m+[m[32m      this.nodes.splice($.inArray(node, this.nodes), 1);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Tree.prototype.render = function() {[m
[32m+[m[32m      var root, _i, _len, _ref;[m
[32m+[m[32m      _ref = this.roots;[m
[32m+[m[32m      for (_i = 0, _len = _ref.length; _i < _len; _i++) {[m
[32m+[m[32m        root = _ref[_i];[m
[32m+[m
[32m+[m[32m        // Naming is confusing (show/render). I do not call render on node from[m
[32m+[m[32m        // here.[m
[32m+[m[32m        root.show();[m
[32m+[m[32m      }[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.sortBranch = function(node, sortFun) {[m
[32m+[m[32m      // First sort internal array of children[m
[32m+[m[32m      node.children.sort(sortFun);[m
[32m+[m
[32m+[m[32m      // Next render rows in correct order on page[m
[32m+[m[32m      this._sortChildRows(node);[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype.unloadBranch = function(node) {[m
[32m+[m[32m      var children, i;[m
[32m+[m
[32m+[m[32m      for (i = 0; i < node.children.length; i++) {[m
[32m+[m[32m        this.removeNode(node.children[i]);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Reset node's collection of children[m
[32m+[m[32m      node.children = [];[m
[32m+[m
[32m+[m[32m      node.updateBranchLeafClass();[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Tree.prototype._moveRows = function(node, destination) {[m
[32m+[m[32m      var children = node.children, i;[m
[32m+[m
[32m+[m[32m      node.row.insertAfter(destination.row);[m
[32m+[m[32m      node.render();[m
[32m+[m
[32m+[m[32m      // Loop backwards through children to have them end up on UI in correct[m
[32m+[m[32m      // order (see #112)[m
[32m+[m[32m      for (i = children.length - 1; i >= 0; i--) {[m
[32m+[m[32m        this._moveRows(children[i], node);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Special _moveRows case, move children to itself to force sorting[m
[32m+[m[32m    Tree.prototype._sortChildRows = function(parentNode) {[m
[32m+[m[32m      return this._moveRows(parentNode, parentNode);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return Tree;[m
[32m+[m[32m  })();[m
[32m+[m
[32m+[m[32m  // jQuery Plugin[m
[32m+[m[32m  methods = {[m
[32m+[m[32m    init: function(options, force) {[m
[32m+[m[32m      var settings;[m
[32m+[m
[32m+[m[32m      settings = $.extend({[m
[32m+[m[32m        branchAttr: "ttBranch",[m
[32m+[m[32m        clickableNodeNames: false,[m
[32m+[m[32m        column: 0,[m
[32m+[m[32m        columnElType: "td", // i.e. 'td', 'th' or 'td,th'[m
[32m+[m[32m        expandable: false,[m
[32m+[m[32m        expanderTemplate: "<a href='#'>&nbsp;</a>",[m
[32m+[m[32m        indent: 19,[m
[32m+[m[32m        indenterTemplate: "<span class='indenter'></span>",[m
[32m+[m[32m        initialState: "collapsed",[m
[32m+[m[32m        nodeIdAttr: "ttId", // maps to data-tt-id[m
[32m+[m[32m        parentIdAttr: "ttParentId", // maps to data-tt-parent-id[m
[32m+[m[32m        stringExpand: "Expand",[m
[32m+[m[32m        stringCollapse: "Collapse",[m
[32m+[m
[32m+[m[32m        // Events[m
[32m+[m[32m        onInitialized: null,[m
[32m+[m[32m        onNodeCollapse: null,[m
[32m+[m[32m        onNodeExpand: null,[m
[32m+[m[32m        onNodeInitialized: null[m
[32m+[m[32m      }, options);[m
[32m+[m
[32m+[m[32m      return this.each(function() {[m
[32m+[m[32m        var el = $(this), tree;[m
[32m+[m
[32m+[m[32m        if (force || el.data("treetable") === undefined) {[m
[32m+[m[32m          tree = new Tree(this, settings);[m
[32m+[m[32m          tree.loadRows(this.rows).render();[m
[32m+[m
[32m+[m[32m          el.addClass("treetable").data("treetable", tree);[m
[32m+[m
[32m+[m[32m          if (settings.onInitialized != null) {[m
[32m+[m[32m            settings.onInitialized.apply(tree);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return el;[m
[32m+[m[32m      });[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    destroy: function() {[m
[32m+[m[32m      return this.each(function() {[m
[32m+[m[32m        return $(this).removeData("treetable").removeClass("treetable");[m
[32m+[m[32m      });[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    collapseAll: function() {[m
[32m+[m[32m      this.data("treetable").collapseAll();[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    collapseNode: function(id) {[m
[32m+[m[32m      var node = this.data("treetable").tree[id];[m
[32m+[m
[32m+[m[32m      if (node) {[m
[32m+[m[32m        node.collapse();[m
[32m+[m[32m      } else {[m
[32m+[m[32m        throw new Error("Unknown node '" + id + "'");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    expandAll: function() {[m
[32m+[m[32m      this.data("treetable").expandAll();[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    expandNode: function(id) {[m
[32m+[m[32m      var node = this.data("treetable").tree[id];[m
[32m+[m
[32m+[m[32m      if (node) {[m
[32m+[m[32m        if (!node.initialized) {[m
[32m+[m[32m          node._initialize();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        node.expand();[m
[32m+[m[32m      } else {[m
[32m+[m[32m        throw new Error("Unknown node '" + id + "'");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    loadBranch: function(node, rows) {[m
[32m+[m[32m      var settings = this.data("treetable").settings,[m
[32m+[m[32m          tree = this.data("treetable").tree;[m
[32m+[m
[32m+[m[32m      // TODO Switch to $.parseHTML[m
[32m+[m[32m      rows = $(rows);[m
[32m+[m
[32m+[m[32m      if (node == null) { // Inserting new root nodes[m
[32m+[m[32m        this.append(rows);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        var lastNode = this.data("treetable").findLastNode(node);[m
[32m+[m[32m        rows.insertAfter(lastNode.row);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.data("treetable").loadRows(rows);[m
[32m+[m
[32m+[m[32m      // Make sure nodes are properly initialized[m
[32m+[m[32m      rows.filter("tr").each(function() {[m
[32m+[m[32m        tree[$(this).data(settings.nodeIdAttr)].show();[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      if (node != null) {[m
[32m+[m[32m        // Re-render parent to ensure expander icon is shown (#79)[m
[32m+[m[32m        node.render().expand();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    move: function(nodeId, destinationId) {[m
[32m+[m[32m      var destination, node;[m
[32m+[m
[32m+[m[32m      node = this.data("treetable").tree[nodeId];[m
[32m+[m[32m      destination = this.data("treetable").tree[destinationId];[m
[32m+[m[32m      this.data("treetable").move(node, destination);[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    node: function(id) {[m
[32m+[m[32m      return this.data("treetable").tree[id];[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    removeNode: function(id) {[m
[32m+[m[32m      var node = this.data("treetable").tree[id];[m
[32m+[m
[32m+[m[32m      if (node) {[m
[32m+[m[32m        this.data("treetable").removeNode(node);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        throw new Error("Unknown node '" + id + "'");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    reveal: function(id) {[m
[32m+[m[32m      var node = this.data("treetable").tree[id];[m
[32m+[m
[32m+[m[32m      if (node) {[m
[32m+[m[32m        node.reveal();[m
[32m+[m[32m      } else {[m
[32m+[m[32m        throw new Error("Unknown node '" + id + "'");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    sortBranch: function(node, columnOrFunction) {[m
[32m+[m[32m      var settings = this.data("treetable").settings,[m
[32m+[m[32m          prepValue,[m
[32m+[m[32m          sortFun;[m
[32m+[m
[32m+[m[32m      columnOrFunction = columnOrFunction || settings.column;[m
[32m+[m[32m      sortFun = columnOrFunction;[m
[32m+[m
[32m+[m[32m      if ($.isNumeric(columnOrFunction)) {[m
[32m+[m[32m        sortFun = function(a, b) {[m
[32m+[m[32m          var extractValue, valA, valB;[m
[32m+[m
[32m+[m[32m          extractValue = function(node) {[m
[32m+[m[32m            var val = node.row.find("td:eq(" + columnOrFunction + ")").text();[m
[32m+[m[32m            // Ignore trailing/leading whitespace and use uppercase values for[m
[32m+[m[32m            // case insensitive ordering[m
[32m+[m[32m            return $.trim(val).toUpperCase();[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          valA = extractValue(a);[m
[32m+[m[32m          valB = extractValue(b);[m
[32m+[m
[32m+[m[32m          if (valA < valB) return -1;[m
[32m+[m[32m          if (valA > valB) return 1;[m
[32m+[m[32m          return 0;[m
[32m+[m[32m        };[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.data("treetable").sortBranch(node, sortFun);[m
[32m+[m[32m      return this;[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    unloadBranch: function(node) {[m
[32m+[m[32m      this.data("treetable").unloadBranch(node);[m
[32m+[m[32m      return this;[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
