[32m+[m[32m    if (methods[method]) {[m
[32m+[m[32m      return methods[method].apply(this, Array.prototype.slice.call(arguments, 1));[m
[32m+[m[32m    } else if (typeof method === 'object' || !method) {[m
[32m+[m[32m      return methods.init.apply(this, arguments);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return $.error("Method " + method + " does not exist on jQuery.treetable");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
